<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mye_wallet">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/dompetku1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dompetku1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".menu_utama">
        </activity>
        <activity android:name=".transfer_activity">
        </activity>
        <activity android:name=".login_activity">
        </activity>
        <activity android:name=".regis_activity">
        </activity>
        <activity android:name=".profil_activity">
        </activity>
        <activity android:name=".edit_profil_activity">
        </activity>
        <activity android:name=".topup_activity">
        </activity>
        <activity android:name=".transfer_nominal_activity">
        </activity>
        <activity android:name=".history_activity">
        </activity>
    </application>

</manifest>